FUNCTION {article}
{ output.bibitem
  format.authors "author" output.check
  new.block
  format.title "title" output.check
  new.sentence
  crossref missing$
    { journal emphasize "journal" output.check
      format.vol.num output
      format.date "year" output.check
      format.pages output
    }
    { format.article.crossref output.nonnull
      format.pages output
    }
